<section class="featured-articles-section flex-column ">
<h2 class="section-title">Featured Articles</h2>
<section class="featured-articles-container flex-row">
  <% featuredArticles.forEach(article => { %>
  <a href="articles/<%= article.slug %>" class="article flex-column small-box">   
    <h3 class="article-title"><%= article.title %></h3>
    <p class="article-title"><%= article.description %></p>
  </a>
  <% }) %>
</section>
</section>